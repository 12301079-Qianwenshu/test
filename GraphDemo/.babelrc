{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": "commonjs"
            }
        ],
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-syntax-dynamic-import",
        [
            "import",
            {
                // "libraryName": "antd",
                // "libraryDirectory": "es",
                // "style": "css" // `style: true` 会加载 less 文件
                "libraryName": "antd",
                "style": "css"
            }
        ]
    ]
}